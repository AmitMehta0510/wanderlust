<% if(success && success.length){ %>
<div
  class="alert alert-success alert-dismissable fade show col-6 offset-6"
  role="alert"
>
  <%= success %>
  <div
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></div>
</div>

<% } %> <% if(error && error.length){ %>
<div
  class="alert alert-danger alert-dismissable fade show col-6 offset-6"
  role="alert"
>
  <%= error %>
  <div
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></div>
</div>
<% } %>
